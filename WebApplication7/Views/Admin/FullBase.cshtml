@model List<Client>
@{
    ViewData["Title"] = "FullBase";
}

<h1>CitiesAreFull</h1>

<table border="1">
    <tr>
        <th>Id</th>
        <th>Имя</th>
        <th>Фамилия</th>
        <th>На счету</th>
        <th>Город</th>
        <th>Персональный код</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id </td>
            <td>@item.Name </td>
            <td>@item.Surname </td>
            <td>
                <form action="~/Admin/ClientInformation" method="get">
                    <input type="text" name="OwnerCode" value="@item.OwnerCode" style="visibility: hidden" />
                    <button type="submit">Remove</button>
                </form>
            </td>

        </tr>
        
    }

</table>
